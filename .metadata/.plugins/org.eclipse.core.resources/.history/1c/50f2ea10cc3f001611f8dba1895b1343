document.writeln('Hello, world!');

var myObject = {
		value: 0,
		getValue: function(){
			return this.value;
		},
		
		double: function(){
			var that = this;
			
			var helper = function(){
				that.value = add(that.value, that.value);
			}
			
			helper();
		},
		
		increment: function(_increment){
			this.value += typeof _increment === 'number' ? _increment : 1;
		}
}

myObject.increment();
document.writeln(myObject.value);

myObject.increment(2);
document.writeln(myObject.value);

myObject.double();
document.writeln('Value: ' + myObject.getValue());

var quo = new Quo('confused');
document.writeln(quo.get_status());

document.writeln(sum3(4,8,15,16,23,42));

(function(){
	try{
		add("seven");
	}
	catch (e){
		document.writeln(e.name + ':' + e.message);
	}
}());

document.writeln((-10/3).integer());
document.writeln('"' + "         neat       ".trim() + '"');

