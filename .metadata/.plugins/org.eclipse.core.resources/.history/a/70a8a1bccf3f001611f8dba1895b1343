console.log("Number", Number);
Number.method('integer', function(){
	var which = this < 0 ? 'ceil' : 'floor';
	return Math[which](this);
});
console.log('Number.method', Number.method);
console.log('Number.integer', Number..integer);
console.log("Number(someNumber)", Number(someNumber));
document.writeln(Number(someNumber).integer());
