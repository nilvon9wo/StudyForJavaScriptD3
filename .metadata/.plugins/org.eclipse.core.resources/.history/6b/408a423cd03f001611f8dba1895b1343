Function.prototype.method = function(name, func){
	console.log("##### name", name);
	console.log("##### func", func);
	console.log("##### this", this);
	console.log("##### this.prototype", this.prototype);
	console.log("##### this.prototype", this.prototype);
	if (!this.prototype[name]){
		this.prototype[name] = func;
		return this;
	}
}