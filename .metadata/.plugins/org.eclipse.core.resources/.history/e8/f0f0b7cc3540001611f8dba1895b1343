var sampleObject = function(){
		var value = 0;
		
		return {
			getValue: function(){
				return value;
			},
			double: function(){
				var that = this;
				var helper = function(){
					that.value = add(that.value, that.value);
				}
				
				helper();
			},
			increment: function(_increment){
				value += typeof _increment === 'number' ? _increment : 1;
			}
		};
}();